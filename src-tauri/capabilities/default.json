{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "http:default",
    "fs:allow-desktop-read-recursive",
    "fs:allow-desktop-write-recursive",
    "dialog:allow-open",
    "dialog:allow-save",
    "sql:default",
    "store:default",
    "store:default"
  ]
}